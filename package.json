{
  "name": "react-native-a11y-highlighter",
  "version": "0.1.0",
  "description": "DEV-only visual overlay that highlights accessibility issues in React Native apps",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "scripts": {
    "prepare": "bob build",
    "typecheck": "tsc --noEmit",
    "clean": "del-cli lib"
  },
  "keywords": [
    "react-native",
    "accessibility",
    "a11y",
    "dev-tools",
    "overlay",
    "touch-target"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gardouhkhalil-afk/react-native-a11y-highlighter.git"
  },
  "license": "MIT",
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-native": ">=0.70.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "del-cli": "^5.0.0",
    "react": "^18.2.0",
    "react-native": "^0.73.0",
    "react-native-builder-bob": "^0.23.0",
    "typescript": "^5.3.0"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  }
}
